           ind(3) + "state.IN_CIRCLE = true;\n" +
           attr.tracePrintCycleBeginString() +
           attr.cacheInitRewrite(ind(3)) +
           attr.addAddToComponent() +
  // TODO: fixme         ind(3) + "state().CIRCLE_INDEX = 1;\n" +
